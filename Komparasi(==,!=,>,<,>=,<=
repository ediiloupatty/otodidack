#include <iostream>

using namespace std;

int main()
{
    cout<<"Nama  : Edi "<<endl;
    cout<<"Kelas : 2Ti5"<<endl;
    cout<<"NIM   : 22024169"<<endl;

    int a=2;
    int b=5;

    bool hasil1, hasil2;

    //komparasi

    //sebanding
    hasil1 =(a==b);
    //tidak sebanding
    hasil2 =(a!=b);
    //lebih dari
    hasil1 = (a>b);
    //kurang dari
    hasil2 =(a<b);
    //lebih dari sama dengan
    hasil1=(a>=b);//1
    //kurang dari sama dengan
    hasil1=(a<=b);//1

    cout<<hasil1<<endl;
    cout<<hasil2<<endl;



    return 0;
}
